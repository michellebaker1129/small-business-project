{
  "name": "client-portal-project",
  "version": "0.1.0",
  "description": "The client and server for the client-portal-project",
  "main": "index.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"cd server && npm run dev\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JosiahRooney/client-portal.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JosiahRooney/client-portal/issues"
  },
  "homepage": "https://github.com/JosiahRooney/client-portal#readme",
  "dependencies": {
    "concurrently": "^8.0.1"
  }
}
